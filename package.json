{
  "name": "semantic-release-gitmoji",
  "version": "1.3.2",
  "description": "Different from conventional changelog, Gitmoji commits are used to determine a release type and generate release notes.",
  "main": "index.js",
  "scripts": {
    "release": "semantic-release",
    "commit": "gitmoji -c",
    "lint": "eslint ./index.js ./lib/**/*.js",
    "link": "npm link ../semantic-release-gitmoji --no-save",
    "test": "ava test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momocow/semantic-release-gitmoji.git"
  },
  "keywords": [
    "changelog",
    "commit-analyzer",
    "github",
    "publish",
    "release",
    "semantic-release",
    "gitmoji"
  ],
  "author": "MomoCow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/momocow/semantic-release-gitmoji/issues"
  },
  "homepage": "https://github.com/momocow/semantic-release-gitmoji#readme",
  "devDependencies": {
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/github": "^5.2.1",
    "@semantic-release/npm": "^5.1.14",
    "ava": "^1.4.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gitmoji-cli": "^1.9.2",
    "lodash.set": "^4.3.2",
    "lodash.shuffle": "^4.2.0",
    "semantic-release": "^15.13.18",
    "sinon": "^7.1.1"
  },
  "dependencies": {
    "dateformat": "^3.0.3",
    "debug": "^4.1.0",
    "emoji-regex": "^7.0.1",
    "git-url-parse": "^11.1.1",
    "handlebars": "^4.1.2",
    "issue-regex": "^2.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.1",
    "lodash.uniq": "^4.5.0",
    "node-emoji": "^1.8.1"
  },
  "peerDependencies": {
    "semantic-release": "^15.0.0"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false
  }
}
